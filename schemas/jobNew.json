{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "minimum": 0,
            "maximum": 1.0
        },
        "company_handle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 25
        }
    },
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "additionalProperties": false
}